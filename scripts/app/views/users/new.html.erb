<div id="cd-login" class="user-form">
<h1>Sign up for this arbitrary app that does nothing!</h1>
  <%= form_for @user, url: users_path do |f| %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Sign up" %>

  <% end %>

</div>
